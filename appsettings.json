{
  "SwaggerConfiguration": [
    {
      "Version": "v1",
      "UIEndpoint": "/swagger/v1/swagger.json"
    }
  ],
  "Logging": {
    "File": {
      "ApplicationName": "CreditOne",
      "FileName": "Sso.API",
      "Extension": "log",
      "LogDirectory": "D:\\LogFiles",
      "RetainedFileCountLimit": 6,
      "FileSizeLimit": 10485760,
      "LogLevel": {
        "Application": "Error",
        "System": "Error",
        "Microsoft": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
